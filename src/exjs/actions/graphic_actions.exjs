defmodule Actions.GraphicActions do
  def startPainting() do
    App.Dispatcher.startPainting()
  end

  def stopPainting() do
    App.Dispatcher.stopPainting()
  end

  def addPoint(data) do
    App.Dispatcher.addPoint(data)
  end

  def changeColor(color) do
    App.Dispatcher.changeColor(color)
  end

  def changeSize(size) do
    App.Dispatcher.changeSize(size)
  end
end
